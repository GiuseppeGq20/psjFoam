Tinlet          300;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
    "(outlet|side)"
    {
        //type            zeroGradient;
        type            fixedValue;
        value   uniform $Tinlet;
    }
    
    "(plane|container|elecSide.*)"
    {
        type            zeroGradient;
    }
    
    elecFace1
    {
        type            zeroGradient;
    }
    
    elecFace2
    {
        type            zeroGradient;
    }
    
    //#includeEtc "caseDicts/setConstraintTypes"
