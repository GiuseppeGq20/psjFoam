FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      source;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * /


dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {   
        type            calculated;
        value           uniform 0;
    }

    "(plane|container|elec.*)"
    {
        type            calculated;
        value           uniform 0;
    }

}

